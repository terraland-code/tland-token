{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StakedResponse",
  "type": "object",
  "required": [
    "denom",
    "stake"
  ],
  "properties": {
    "denom": {
      "type": "string"
    },
    "stake": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
